{
  "name": "interop",
  "private": true,
  "devDependencies": {
    "@eslint/js": "^9.5.0",
    "@inrupt/base-rollup-config": "^3.1.0",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^20.8.6",
    "@vitest/coverage-v8": "^1.6.0",
    "componentsjs-generator": "^3.1.2",
    "eslint": "^9.5.0",
    "eslint-plugin-vue": "^9.26.0",
    "husky": "^8.0.3",
    "jest-rdf": "^1.8.0",
    "lint-staged": "^15.0.1",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "rollup": "^4.13.0",
    "tslib": "^2.6.3",
    "turbo": "^1.10.16",
    "turbowatch": "^2.29.4",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.13.0",
    "vitest": "^1.6.0"
  },
  "scripts": {
    "prepare": "husky install",
    "build": "turbo run build",
    "test": "turbo run test --concurrency=1",
    "clean": "turbo run clean",
    "dev": "turbo run dev",
    "watch": "./node_modules/.bin/turbowatch turbowatch.ts",
    "lint": "eslint"
  },
  "volta": {
    "node": "20.7.0"
  },
  "packageManager": "pnpm@9.0.2",
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint",
      "git add"
    ],
    "*.vue": [
      "eslint",
      "git add"
    ]
  },
  "pnpm": {
    "overrides": {
      "rollup": "npm:@rollup/wasm-node"
    },
    "supportedArchitectures": {
      "os": ["win32", "darwin", "current", "linux"],
      "cpu": ["x64", "arm64"]
    }
  }
}
