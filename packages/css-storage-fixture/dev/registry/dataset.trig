PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX acl: <http://www.w3.org/ns/auth/acl#>
PREFIX acp: <http://www.w3.org/ns/solid/acp#>
PREFIX interop: <http://www.w3.org/ns/solid/interop#>
PREFIX ldp: <http://www.w3.org/ns/ldp#>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX oidc: <http://www.w3.org/ns/solid/oidc#>

GRAPH <https://reg.docker/> {
  <https://reg.docker/>
    ldp:contains
      <https://reg.docker/alice/>,
      <https://reg.docker/bob/>,
      <https://reg.docker/kim/> .
}

GRAPH <meta:https://reg.docker/alice/> {
  <https://reg.docker/alice/>
    a interop:RegistrySet, ldp:Resource;
    interop:hasAgentRegistry <https://reg.docker/alice/agent/>;
    interop:hasAuthorizationRegistry <https://reg.docker/alice/authorization/>;
    interop:hasDataRegistry
      <https://alice-home.data.docker/>,
      <https://alice-work.data.docker/>.
}

GRAPH <meta:https://reg.docker/alice/authorization/> {
  <https://reg.docker/alice/authorization/>
    a interop:AuthorizationRegistry, ldp:Resource.
}

GRAPH <meta:https://reg.docker/alice/agent/> {
  <https://reg.docker/alice/agent/>
    a interop:AgentRegistry, ldp:Resource;
    interop:hasApplicationRegistration 
      <https://reg.docker/alice/agent/cvmsa4/>;
    interop:hasSocialAgentRegistration
      <https://reg.docker/alice/agent/cp9g7p/>,
      <https://reg.docker/alice/agent/vzp6ky/>.
}

GRAPH <meta:https://reg.docker/alice/agent/cp9g7p/> {
  <https://reg.docker/alice/agent/cp9g7p/>
    a interop:SocialAgentRegistration, ldp:Resource;
    interop:registeredAgent <https://acme.id.docker> ;
    #interop:reciprocalRegistration <https://reg.docker/acme/agent/bgtrtf/> ;
    skos:prefLabel "ACME" ;
    skos:note "A producer of open source gadget. We are collaborating on some software projects" .
}

GRAPH <meta:https://reg.docker/alice/agent/vzp6ky/> {
  <https://reg.docker/alice/agent/vzp6ky/>
    a interop:SocialAgentRegistration, ldp:Resource;
    interop:registeredAgent <https://bob.id.docker> ;
    interop:reciprocalRegistration <https://reg.docker/bob/agent/jc5gt6/> ;
    skos:prefLabel "Bob" ;
    skos:note "A colleague at ACME" .
}

GRAPH <meta:https://reg.docker/alice/agent/cvmsa4/> {
  <https://reg.docker/alice/agent/cvmsa4/>
    a interop:ApplicationRegistration, ldp:Resource;
    interop:registeredAgent <https://vuejectron.docker/id>;
    interop:hasAccessGrant <https://reg.docker/alice/agent/cvmsa4/y1pp6y>.
  <https://vuejectron.docker/id> oidc:client_name "Vuejectron";
      oidc:logo_uri <https://robohash.org/aHR0cHM6Ly92dWVqZWN0cm9uLmRvY2tlcg?set=set3>;
      interop:hasAccessNeedGroup <https://vuejectron.docker/access-needs#need-group-pm>;
      interop:hasAuthorizationCallbackEndpoint <https://vuejectron.docker>.
}

GRAPH <https://reg.docker/alice/agent/cvmsa4/> {
  <https://reg.docker/alice/agent/cvmsa4/>
    ldp:contains
      <https://reg.docker/alice/agent/cvmsa4/y1pp6y>,
      <https://reg.docker/alice/agent/cvmsa4/afiooi>.
}

GRAPH <https://reg.docker/alice/agent/cvmsa4/y1pp6y> {
  <https://reg.docker/alice/agent/cvmsa4/y1pp6y>
    a interop:AccessGrant;
    interop:granted true;
    interop:grantee <https://vuejectron.docker/id>;
    interop:hasAccessNeedGroup <https://vuejectron.docker/access-needs#need-group-pm>;
    interop:hasDataGrant
      <https://reg.docker/alice/agent/cvmsa4/afiooi>.
}

GRAPH <meta:https://reg.docker/alice/agent/cvmsa4/y1pp6y> {
        <https://reg.docker/alice/agent/cvmsa4/y1pp6y> a ldp:Resource .
}

GRAPH <https://reg.docker/alice/agent/cvmsa4/afiooi> {
  <https://reg.docker/alice/agent/cvmsa4/afiooi>
    a interop:DataGrant;
    interop:grantee <https://vuejectron.docker/id>;
    interop:dataOwner <https://alice.id.docker>;
    interop:registeredShapeTree <https://shapetrees.data.docker/trees/Project>;
    interop:hasDataRegistration <https://alice-work.data.docker/sx86hi/>;
    interop:scopeOfGrant interop:AllFromRegistry;
    interop:accessMode acl:Read, acl:Create, acl:Update, acl:Delete.
}

GRAPH <meta:https://reg.docker/alice/agent/cvmsa4/afiooi> {
        <https://reg.docker/alice/agent/cvmsa4/afiooi> a ldp:Resource .
}

GRAPH <meta:https://reg.docker/alice/.acr> {
        <https://reg.docker/alice/.acr> a ldp:Resource .
}

GRAPH <https://reg.docker/alice/.acr> {
  <https://reg.docker/alice/.acr#root>
    a acp:AccessControlResource;
    acp:resource <https://reg.docker/alice/>;
    acp:accessControl
      <https://reg.docker/alice/.acr#publicReadAccess>,
      <https://reg.docker/alice/.acr#fullOwnerAccess>;
    acp:memberAccessControl
      <https://reg.docker/alice/.acr#publicReadAccess>,
      <https://reg.docker/alice/.acr#fullOwnerAccess>.

  <https://reg.docker/alice/.acr#publicReadAccess>
    a acp:AccessControl;
    acp:apply [
      a acp:Policy;
      acp:allow acl:Read;
      acp:anyOf [
        a acp:Matcher;
        acp:agent acp:PublicAgent
      ]
    ].

  <https://reg.docker/alice/.acr#fullOwnerAccess>
    a acp:AccessControl;
    acp:apply [
      a acp:Policy;
      acp:allow acl:Read, acl:Write, acl:Control;
      acp:anyOf [
        a acp:Matcher;
        acp:agent <https://alice.id.docker>
      ]
    ].
}

